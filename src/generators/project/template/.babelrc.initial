{
    "presets": [
        "es2015",
        "stage-0",
        "react"
    ],
    "plugins": [
        "react-hot-loader/babel",
        "transform-runtime",
        "transform-decorators-legacy",
    ],
    "env": {
        production: {
            plugins: [
                "transform-remove-console",
                "transform-remove-debugger",
                "transform-react-inline-elements",
                "transform-react-constant-elements"
            ]
        }
    }
}
